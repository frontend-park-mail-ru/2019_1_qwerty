{
  "version": 2,
  "name": "front",
  "alias": ["frontend-qwerty"],
  "builds": [

      { "src": "src/public/**", "use": "@now/static" }
  ],
  "routes": [
      { "src": "/images/(.+)", "dest": "src/public/images/$1" },
      { "src": "/[^/]*$", "dest": "src/public/index.html" },
      { "src": "/fonts/(.*)", "dest": "src/public/fonts/$1" },
      { "src": "/build/(.+)", "dest":  "src/public/build/$1"}
  ]
}